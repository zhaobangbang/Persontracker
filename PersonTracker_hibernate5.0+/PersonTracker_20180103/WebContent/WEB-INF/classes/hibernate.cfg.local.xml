  <mapping resource="dao_hbm/ProjectManagers.hbm.xml"/>
  <mapping resource="dao_hbm/MapInfo.hbm.xml"/>
  <mapping resource="dao_hbm/FieldInfo.hbm.xml"/>
  <mapping resource="dao_hbm/GatewayInfo.hbm.xml"/>
  <mapping resource="dao_hbm/CompanyInfo.hbm.xml"/>
  <mapping resource="dao_hbm/ConstructInfo.hbm.xml"/>
  <mapping resource="dao_hbm/WorkerInfo.hbm.xml"/>
  <mapping resource="dao_hbm/PositionRecord.hbm.xml"/>
  <mapping resource="dao_hbm/DevInfo.hbm.xml"/> 
  <!--<mapping class="com.lansitec.dao.beans.DevInfo"/>  -->
  
  <mapping resource="dao_hbm/AssetInfo.hbm.xml"/>
  <mapping resource="dao_hbm/DevConfig.hbm.xml"/> 
  <mapping resource="dao_hbm/Beacons.hbm.xml"/>
